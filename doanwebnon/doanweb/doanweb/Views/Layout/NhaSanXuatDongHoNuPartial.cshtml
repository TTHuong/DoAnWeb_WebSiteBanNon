@using doanweb.Models;
@model List<tbl_nhasanxuat>

@if(Model.Count!=0)
{
    <li class="nav-item dropdown">
        <a class="nav-link dropdown-toggle" href="#" id="navbardrop" data-toggle="dropdown">
            NÓN NỮ
        </a>
        <div class="dropdown-menu fontchu">
            @foreach (var item in Model)
            {
                <a class="dropdown-item" href="@Url.Action("XemTatCaSanPhamCuaCungNhaSanXuat", "Layout", new { @NSX=item.Id,@Loai="Nón Nữ"})">@item.TenNhaSanXuat</a>
            }
        </div>
    </li>
}

