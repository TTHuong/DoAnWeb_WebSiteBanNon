@model doanweb.Models.tbl_member

@{
    ViewBag.Title = "Dangky";
    Layout = "~/Views/Layout/_LayoutNguoiDung.cshtml";
}

<div class="limiter" style="margin-top: 4%;">
    <div class="container-login100">
        <div class="wrap-login100 p-l-110 p-r-110 p-t-62 p-b-33">
                <form class="login100-form validate-form flex-sb flex-w" method="post">
                    <span class="login100-form-title p-b-53">
                        ĐĂNG KÝ @ViewBag.tieudedk
                    </span>
                   
                    <a href="@Url.Action("DangKyFacebook", "DangKy")" class="btn-face m-b-20">
                        <img src="~/content/images/icon/icon-facebook.png" alt="facebook" style="margin-right: 20px;">
                        facebook
                    </a>
                    <a href="@Url.Action("DangKyGoogle", "DangKy")" class="btn-google m-b-20">
                        <img src="~/Content/images/icon/icon-google.png" alt="GOOGLE">
                        Google
                    </a>
             </form>
            
            <form class="login100-form validate-form flex-sb flex-w" method="POST" action="@Url.Action("Dangky","DangKy")" name="frmdangky">

                <div class="p-t-31 p-b-9">
                    <span class="txt1">
                        Họ Và Tên @ViewBag.tenkhachhang
                    </span>
                </div>
                <div class="wrap-input100 validate-input" data-validate="Họ Và Tên Không Được Bỏ Trống">
                    @Html.TextBoxFor(model => model.TenKhachHang, new { @class = "input100", @type = "text", @name = "yourname", @id = "yourname", @placeholder = "Họ và Tên", @value = "" })

                    @*<input class="input100" type="text" name="yourname" id="yourname" placeholder="Họ và Tên">*@
                    <span class="focus-input100"></span>
                </div>
                <div class="p-t-31 p-b-9">
                    <span class="txt1">
                        Ngày Sinh @ViewBag.ngaysinh
                    </span>
                </div>
                <div class="wrap-input100 validate-input" data-validate="Ngày Sinh Không Được Bỏ Trống">
                    @Html.TextBoxFor(model => model.NgaySinh, new { @class = "input100", @type = "date", @name = "birthday", @id = "birthday", @min = "1970-01-01", @value = "" })

                    @*<input class="input100" type="date" name="birthday" id="birthday">*@
                    <span class="focus-input100"></span>
                </div>
                <div class="p-t-31 p-b-9">
                    <span class="txt1">
                        Tên Đăng Nhập  @ViewBag.tendangnhap
                    </span>
                </div>
                <div class="wrap-input100 validate-input" data-validate="Tên Đăng Nhập Không Được Bỏ Trống">
                    @Html.TextBoxFor(model => model.TenDangNhap, new { @class = "input100", @type = "text", @placeholder = "Tên Đăng Nhập", @name = "username", @id = "username", @value = "" })

                    @*<input class="input100" type="text" placeholder="Tên Đăng Nhập" name="username" id="username">*@
                    <span class="focus-input100"></span>
                </div>

                <div class="p-t-13 p-b-9">
                    <span class="txt1">
                        Mật Khẩu @ViewBag.matkhau
                    </span>
                </div>
                <div class="wrap-input100 validate-input" data-validate="Mật Khẩu Không Được Bỏ Trống">
                    @Html.TextBoxFor(model => model.MatKhau, new { @class = "input100", @type = "password", @placeholder = "Mật Khẩu", @name = "password", @id = "password", @value = "" })

                    @*<input class="input100" type="password" placeholder="Mật Khẩu" name="password" id="password">*@
                    <span class="focus-input100"></span>
                </div>

                <div class="p-t-13 p-b-9">
                    <span class="txt1">
                        Email @ViewBag.email
                    </span>
                </div>
                <div class="wrap-input100 validate-input" data-validate="Email Không Được Bỏ Trống">
                    @Html.TextBoxFor(model => model.Email, new { @class = "input100", @type = "email", @placeholder = "VD: NguyenVanA@gmail.com", @name = "email", @id = "email", @value = "" })

                    @*<input class="input100" type="email" placeholder="VD: NguyenVanA@gmail.com" name="email" id="email">*@
                    <span class="focus-input100"></span>
                </div>
                <div class="p-t-13 p-b-9">
                    <span class="txt1">
                        Số Điện Thoại @ViewBag.sdt
                    </span>
                </div>
                <div class="wrap-input100 validate-input" data-validate="Số Điện Thoại Không Được Bỏ Trống">
                    @Html.TextBoxFor(model => model.SDT, new { @class = "input100", @type = "tel", @placeholder = "VD: 0905974281", @name = "number", @id = "number", @maxlength = "11", @minlenght = "10", @value = "" })

                    @*<input class="input100" type="text" placeholder="VD: 0905974281" name="number" id="number">*@
                    <span class="focus-input100"></span>
                </div>
                <div class="p-t-13 p-b-9">
                    <span class="txt1">
                        Địa Chỉ @ViewBag.diachi
                    </span>
                </div>
                <div class="wrap-input100 validate-input" data-validate="Đại Chỉ Không Được Bỏ Trống">
                    @Html.TextAreaFor(model => model.DiaChi, new { @class = "input100", @type = "text", @placeholder = "VD: Thành Phố ,Quận ,Huyện,Xã ,Ấp...", @name = "address", @id = "gender", @value = "" })

                    @*<input class="input100" type="text" placeholder="VD: Thành Phố ,Quận ,Huyện,Xã ,Ấp..." name="address" id="gender">*@
                    <span class="focus-input100"></span>
                </div>
                <div class="container-login100-form-btn m-t-17">
                    <button class="login100-form-btn" type="submit" id="sbdangky" value="">
                        Đăng Ký
                    </button>
                </div>
            </form>
        </div>
    </div>
</div>
<div id="dropDownSelect1"></div>